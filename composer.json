{
    "name": "elvanto/omnipay-tithely",
    "description": "Tithe.ly driver for the Omnipay payment processing library",
    "keywords": [
        "omnipay",
        "tithely"
    ],
    "homepage": "https://github.com/elvanto/omnipay-tithely",
    "license": "MIT",
    "require": {
        "php" : ">=7.4.0",
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "scrutinizer/ocular": "~1.1",
        "vimeo/psalm": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Tithely\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Tithely\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
